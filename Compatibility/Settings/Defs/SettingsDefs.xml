<?xml version="1.0" encoding="utf-8"?>
<Defs>

	<XmlExtensions.SettingsMenuDef>
		<defName>MaleRatkinSettings</defName> <!--Can be anything unique with no periods-->
		<modId>MaleRatkin</modId> <!--MUST be the same as the one entered earlier-->    
		<label>Male Ratkin</label> <!-- This is what displays as your mod's name-->    
		<settings> <!--This is where you define what settings are in the settings menu-->
			<li Class="XmlExtensions.Setting.Slider">
				<key>MaleRatkinProbSlider</key>
				<label>Male Ratkin: {MaleRatkinProbSlider}. The chance for a male Ratkin to spawn, from 0 (0%) to 1 (100%). </label>
				<min>0.00</min>
				<max>1.00</max>
				<decimals>2</decimals>
			</li>
			<li Class="XmlExtensions.Setting.ResetSettings"/>
		</settings>
	</XmlExtensions.SettingsMenuDef>

</Defs>